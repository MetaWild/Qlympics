{
  "name": "@qlympics/api",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "start:worker": "node dist/worker.js",
    "migrate": "tsx src/scripts/migrate.ts",
    "seed:game-mode": "tsx src/scripts/seedGameMode.ts",
    "quai:ping": "tsx scripts/quai-ping.ts",
    "quai:derive-key": "node scripts/quai-derive-private-key.js",
    "quai:balance": "tsx scripts/quai-balance.ts",
    "test": "tsx --test --test-concurrency=1 test/**/*.test.ts",
    "lint": "node -e \"console.log('no lint config yet')\""
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "pg": "^8.12.0",
    "quais": "^1.0.0-alpha.53",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
